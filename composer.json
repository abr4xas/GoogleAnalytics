{
    "name": "abr4xas/googleanalytics",
    "type": "library",
    "description": "Google Analytics for Laravel",
    "keywords": [
        "abr4xas",
        "GoogleAnalytics"
    ],
    "homepage": "https://github.com/abr4xas/googleanalytics",
    "license": "MIT",
    "authors": [{
            "name": "Shinichi Okada",
            "email": "okada.shin@gmail.com",
            "homepage": "https://github.com/shinokada",
            "role": "Developer"
        },
        {
            "name": "Angel Cruz",
            "email": "me@abr4xas.org",
            "homepage": "https://github.com/abr4xas",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.4",
        "php": "~5.6|~7.0",
        "spatie/laravel-analytics": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0||~5.0||~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Abr4xas\\GoogleAnalytics\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abr4xas\\GoogleAnalytics\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Abr4xas\\GoogleAnalytics\\GoogleAnalyticsServiceProvider"
            ]
        }

    },
    "config": {
        "sort-packages": true
    }
}
