{
    "name": "shinokada/googleanalytics",
    "type": "library",
    "description": "Google Analytics for Backpack for Laravel",
    "keywords": [
        "shinokada",
        "GoogleAnalytics"
    ],
    "homepage": "https://github.com/shinokada/googleanalytics",
    "license": "MIT",
    "authors": [
        {
            "name": "Shinichi Okada",
            "email": "okada.shin@gmail.com",
            "homepage": "http://www.cecilieo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.4",
        "backpack/crud": "^3.2.0",
        "php" : "~5.6|~7.0",
        "spatie/laravel-analytics": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Shinokada\\GoogleAnalytics\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shinokada\\GoogleAnalytics\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
